    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss \"!**/_normalize.scss\"",
    "eslint": "eslint \"source/**/*.js\" \"gulp/tasks/**/*.js\"",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build:clean && gulp build",
    "dev": "gulp dev",
    "start": "gulp build:clean && npm run dev",
    "start:prod": "cross-env NODE_ENV=production gulp sorting && gulp build:clean && npm run dev",
    "hosting": "cross-env NODE_ENV=production gulp sorting gulp && gulp hosting",
    "zip": "cross-env NODE_ENV=production gulp zip",
    "gzip": "cross-env NODE_ENV=production gulp gzip",
    "deploy": "cross-env NODE_ENV=production npm run build && gh-pages -d build && gulp build:clean",
    "wepback": "wepback -p"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
